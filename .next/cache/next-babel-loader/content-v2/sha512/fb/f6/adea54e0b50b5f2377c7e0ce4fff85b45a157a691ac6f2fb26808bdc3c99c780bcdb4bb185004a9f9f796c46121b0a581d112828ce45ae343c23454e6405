{"ast":null,"code":"import _toConsumableArray from \"@babel/runtime/helpers/esm/toConsumableArray\";\nimport { FETCH_SHOP, ADD_SHOP, DELETE_SHOP, UPDATE_SHOP_DETAIL } from '../constants/ActionTypes';\nexport default function () {\n  var state = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : [];\n  var action = arguments.length > 1 ? arguments[1] : undefined;\n\n  switch (action.type) {\n    case FETCH_SHOP:\n      return _toConsumableArray(action.payload);\n\n    case ADD_SHOP:\n      return [].concat(_toConsumableArray(state), [action.payload]);\n\n    case DELETE_SHOP:\n      var deleteID = action.payload;\n      var shop = state.map(function (obj) {\n        if (obj.idshop !== deleteID) return obj;\n      });\n      var result = shop.filter(function (el) {\n        return el;\n      });\n      return _toConsumableArray(result);\n\n    case UPDATE_SHOP_DETAIL:\n      var data = action.payload;\n      var updatedShop = state.map(function (obj) {\n        if (obj.idshop === data.idshop) {\n          return data;\n        } else {\n          return obj;\n        }\n      });\n      return updatedShop;\n\n    default:\n      return state;\n  }\n}","map":{"version":3,"sources":["C:/Program Files (x86)/Ampps/www/auckland-shop-chatbot-admin/src/reducers/shopList.tsx"],"names":["FETCH_SHOP","ADD_SHOP","DELETE_SHOP","UPDATE_SHOP_DETAIL","state","action","type","payload","deleteID","shop","map","obj","idshop","result","filter","el","data","updatedShop"],"mappings":";AAAA,SAASA,UAAT,EAAqBC,QAArB,EAA+BC,WAA/B,EAA4CC,kBAA5C,QAAsE,0BAAtE;AAEA,eAAe,YAA8B;AAAA,MAApBC,KAAoB,uEAAZ,EAAY;AAAA,MAARC,MAAQ;;AACzC,UAAQA,MAAM,CAACC,IAAf;AACI,SAAKN,UAAL;AACI,gCACOK,MAAM,CAACE,OADd;;AAGJ,SAAKN,QAAL;AACI,0CACOG,KADP,IAEIC,MAAM,CAACE,OAFX;;AAIJ,SAAKL,WAAL;AACI,UAAMM,QAAQ,GAAGH,MAAM,CAACE,OAAxB;AACA,UAAME,IAAI,GAAGL,KAAK,CAACM,GAAN,CAAU,UAACC,GAAD,EAAS;AAC5B,YAAIA,GAAG,CAACC,MAAJ,KAAeJ,QAAnB,EACI,OAAOG,GAAP;AACP,OAHY,CAAb;AAIA,UAAME,MAAM,GAAGJ,IAAI,CAACK,MAAL,CAAY,UAAUC,EA